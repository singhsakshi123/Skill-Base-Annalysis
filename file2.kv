<WelcomeScreen>:
    name: 'welcome'
    canvas.before:
        Color:
            rgba: (1, 1, 1, 1)
        Rectangle:
            source:'./Images/background.jpg'
            size: root.width, root.height
            pos: self.pos
    MDLabel:
        text: 'Welcome to Career Analysis'
        theme_text_color: 'Custom'
        text_color: 1,1,1,1
        font_style: 'Button'
        font_size: '40sp'
        halign: 'center'
        pos_hint: {'center_x': .5, 'center_y': .7}
    MDRoundFlatButton:
        text: 'Login'
        theme_text_color: 'Custom'
        line_color: 1, 1, 1, 1
        text_color: 1,1,1,1
        pos_hint: {'center_x':.3,'center_y':.2}
        font_style: 'Subtitle1'
        on_press:
            root.manager.current = 'login'
            root.manager.transition.direction = 'left'
    MDRoundFlatButton:
        text: 'Signup'
        theme_text_color: 'Custom'
        line_color: 1, 1, 1, 1
        text_color: 1,1,1,1
        pos_hint: {'center_x':.7,'center_y':.2}
        font_style: 'Subtitle1'
        on_press:
            root.manager.current = 'signup'
            root.manager.transition.direction = 'right'

<SignupScreen>:
    name: 'signup'
    canvas.before:
        Color:
            rgba: (1, 1, 1, 1)
        Rectangle:
            source:'./Images/background.jpg'
            size: root.width, root.height
            pos: self.pos
    MDLabel:
        text: 'Signup'
        theme_text_color: 'Custom'
        text_color: 1,1,1,1
        font_style: 'Button'
        font_size: '40sp'
        halign: 'center'
        pos_hint: {'center_x': .5, 'center_y': .9}
    MDTextField:
        id: username_text_field
        theme_text_color: 'Custom'
        text_color: 1,1,1,1
        hint_text: 'Username'
        pos_hint: {'center_x': .5, 'center_y': .7}
        size_hint: (0.7,0.1)
    MDTextField:
        id: email_text_field
        theme_text_color: 'Custom'
        text_color: 1,1,1,1
        hint_text: 'Email'
        pos_hint: {'center_x': .5, 'center_y': .6}
        size_hint: (0.7,0.1)
    MDTextField:
        id: password_text_field
        theme_text_color: 'Custom'
        text_color: 1,1,1,1
        hint_text: 'Password'
        pos_hint: {'center_x': .5, 'center_y': .5}
        size_hint: (0.7,0.1)
        password: True
    MDFloatingActionButton:
        id: signup_fab
        icon: 'check'
        md_bg_color: app.theme_cls.primary_color
        user_font_size: '40sp'
        pos_hint: {'center_x': .5, 'center_y': .3}
        on_press: 
            app.store_username()
            # root.manager.current = 'login'
            # root.manager.transition.direction = 'down'
    MDRoundFlatButton:
        text: 'Back'
        theme_text_color: 'Custom'
        line_color: 1, 1, 1, 1
        text_color: 1,1,1,1
        pos_hint: {'center_x':.2,'center_y':.3}
        font_style: 'Subtitle1'
        on_press:
            root.manager.current = 'welcome'
            root.manager.transition.direction = 'left'
    MDRoundFlatButton:
        text: 'Login'
        theme_text_color: 'Custom'
        line_color: 1, 1, 1, 1
        text_color: 1,1,1,1
        pos_hint: {'center_x':.8,'center_y':.3}
        font_style: 'Subtitle1'
        on_press:
            root.manager.current = 'login'
            root.manager.transition.direction = 'right'
<LoginScreen>:
